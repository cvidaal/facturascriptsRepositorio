    </column>
    <column>
        <name>idalumno</name>
        <type>integer</type>
        <null>NO</null>
    </column>
    <column>
        <name>idasignatura</name>
        <type>integer</type>
        <null>NO</null>
    </column>
    <column>
        <name>fechadealta</name>
        <type>date</type>
        <null>NO</null>
    </column>
    <column>
        <name>fechadebaja</name>
        <type>date</type>
    </column>
    <column>
        <name>cuota</name>
        <type>double precision</type>
