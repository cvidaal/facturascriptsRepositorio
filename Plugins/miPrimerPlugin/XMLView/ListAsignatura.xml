<?xml version='1.0' encoding='UTF-8'?>
<view>
    <columns>
        <column name="Profesor" order="100" numcolumns="2">
            <widget type="autocomplete" fieldname="idprofesor" onclick="EditProfesor">
                <values source="profesores" fieldcode="idprofesor" fieldtitle="nombre"/>
            </widget>
        </column>
        <column name="Nombre" order="100" display="left">
            <widget type="text" fieldname="nombre"/>
        </column>
        <column name="Material" order="110" display="left">
            <widget type="text" fieldname="material"/>
        </column>
        <column name="Plazas" order="120" display="right">
            <widget type="number" fieldname="plazas"/>
        </column>
    </columns>
</view>